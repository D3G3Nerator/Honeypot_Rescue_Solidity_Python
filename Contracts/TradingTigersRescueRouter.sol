contract TradingTigersRescueRouter {
    using SafeMath for uint;

    address public immutable factory;
    address public DEV;

    receive() external payable {}

    modifier onlyAdmin() {
        require(msg.sender == DEV, "admin: wut do you try?");
        _;
    }
    // PANCAKESWAP Factory => 0xcA143Ce32Fe78f1f7019d7d551a6402fC5350c73
    constructor(address _factory) public {
        factory = _factory;
        DEV = msg.sender;
    }


    // Before this function can be used, your tokens must be sent to this created contract.
    // If transfer Fail, we have no chance to sell it.
    // Path = Token address, WBNB/BUSD/USDT or what ever. 
    function HoneypotBypass(uint amountOutMin, address[] calldata path) external virtual onlyAdmin {
        uint256 balance = IERC20(path[0]).balanceOf(address(this));
        swapExactTokensForTokensSupportingFeeOnTransferTokens(balance, amountOutMin, path, DEV);
    }

    function sendBack(address Token) external virtual onlyAdmin {
        uint256 balance = IERC20(Token).balanceOf(address(this));
        IERC20(Token).transfer(DEV, balance);
    }


    function swapExactTokensForTokensSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] memory path,
        address to
    ) internal virtual {
        IERC20(path[0]).transfer(PancakeLibrary.pairFor(factory, path[0], path[1]), amountIn); // There is it, No use TransferFrom!, use transfer!
        uint balanceBefore = IERC20(path[path.length - 1]).balanceOf(to);
        _swapSupportingFeeOnTransferTokens(path, to);
        require(
            IERC20(path[path.length - 1]).balanceOf(to).sub(balanceBefore) >= amountOutMin,
            'amountOutMin_smaller_then_given_value'
        );
    }

    function _swapSupportingFeeOnTransferTokens(address[] memory path, address _to) internal virtual {
        for (uint i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0,) = PancakeLibrary.sortTokens(input, output);
            IPancakePair pair = IPancakePair(PancakeLibrary.pairFor(factory, input, output));
            uint amountInput;
            uint amountOutput;

            { // scope to avoid stack too deep errors
            (uint reserve0, uint reserve1,) = pair.getReserves();
            (uint reserveInput, uint reserveOutput) = input == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
            amountInput = IERC20(input).balanceOf(address(pair)).sub(reserveInput);
            amountOutput = PancakeLibrary.getAmountOut(amountInput, reserveInput, reserveOutput);
            }

            (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOutput) : (amountOutput, uint(0));
            address to = i < path.length - 2 ? PancakeLibrary.pairFor(factory, output, path[i + 2]) : _to;
            pair.swap(amount0Out, amount1Out, to, new bytes(0));
        }
    }

}